{
	"document_template": {
		"prefix": "document",
		"body": [
			"\\input texinfo @c -*-texinfo-*-",
			"@setfilename ${1:filename}",
			"@settitle ${2:program_title version}",
			"@documentdescription",
			"${3:document_description}",
			"@end documentdescription",
			"@documentlanguage ${4:ll[_CC]}",
			"",
			"@copying",
			"${5:program_description}",
			"",
			"Copyright @copyright{} ${CURRENT_YEAR:YYYY} ${6:your_name}",
			"",
			"@quotation",
			"${7:copying_notice}",
			"@end quotation",
			"@end copying",
			"",
			"@titlepage",
			"@title ${8:document_title}",
			"@author ${6}",
			"",
			"@page",
			"@vskip 0pt plus 1filll",
			"@insertcopying",
			"",
			"Published by ${6}",
			"@end titlepage",
			"",
			"@c @shortcontents",
			"@contents",
			"",
			"@ifnottex",
			"@node Top",
			"@top ${8}",
			"${5}",
			"@end ifnottex",
			"",
			"@menu",
			"* $9",
			"* Index:: Complete index.",
			"@end menu",
			"",
			"$0",
			"",
			"@node       Index",
			"@unnumbered Index",
			"@printindex cp",
			"",
			"@bye",
			""
		],
		"description": "Template for a document"
	},

	"@@": {
		"prefix": "@@",
		"body": [ "@@" ],
		"description": "Literal '@' character"
	},
	"@!": {
		"prefix": "@!",
		"body": [ "@!" ],
		"description": "Forced end of sentence with an exclamation mark"
	},
	"@\"": {
		"prefix": "@\"",
		"body": [ "@\"$1" ],
		"description": "Non-ASCII Umlaut Accent"
	},
	"@&": {
		"prefix": "@&",
		"body": [ "@&" ],
		"description": "Literal '&' character (only needed in definition commands)"
	},
	"@'": {
		"prefix": "@'",
		"body": [ "@'$1" ],
		"description": "Non-ASCII Acute Accent"
	},
	"@*": {
		"prefix": "@*",
		"body": [ "@*" ],
		"description": "Force line break"
	},
	"@,": {
		"prefix": "@,{}",
		"body": [ "@,{$1}$0" ],
		"description": "Non-ASCII Cedilla Accent"
	},
	"@-": {
		"prefix": "@-",
		"body": [ "@-" ],
		"description": "Discretionary Hyphen"
	},
	"@.": {
		"prefix": "@.",
		"body": [ "@." ],
		"description": "Forced end of sentence with a period"
	},
	"@/": {
		"prefix": "@/",
		"body": [ "@/" ],
		"description": "Allow line break in printed manual"
	},
	"@:": {
		"prefix": "@:",
		"body": [ "@:" ],
		"description": "Not ending a sentence"
	},
	"@=": {
		"prefix": "@=",
		"body": [ "@=$1" ],
		"description": "Non-ASCII Macron/Overbar Accent"
	},
	"@?": {
		"prefix": "@?",
		"body": [ "@?" ],
		"description": "Forced end of sentence with a question mark"
	},
	"@^": {
		"prefix": "@^",
		"body": [ "@^$1" ],
		"description": "Non-ASCII Circumflex Accent"
	},
	"@`": {
		"prefix": "@`",
		"body": [ "@`$1" ],
		"description": "Non-ASCII Grave Accent"
	},
	"@{": {
		"prefix": "@{",
		"body": [ "@{" ],
		"description": "Literal '{' character"
	},
	"@}": {
		"prefix": "@}",
		"body": [ "@}" ],
		"description": "Literal '}' character"
	},
	"@~": {
		"prefix": "@~",
		"body": [ "@~$1" ],
		"description": "Non-ASCII Tilde Accent"
	},
	"@aa": {
		"prefix": "@aa{}",
		"body": [ "@aa{}" ],
		"description": "Non-ASCII uppercase 'A' with circle: 'å'"
	},
	"@AA": {
		"prefix": "@AA{}",
		"body": [ "@AA{}" ],
		"description": "Non-ASCII lowercase 'A' with circle: 'Å'"
	},
	"@abbr": {
		"prefix": "@abbr{}",
		"body": [ "@abbr{${1:abbrevation}${2:[, meaning]}}" ],
		"description": "General abbrevation"
	},
	"@acronym": {
		"prefix": "@acronym{}",
		"body": [ "@acronym{${1:acronym}${2:[, meaning]}}" ],
		"description": "Abbrevations written in all capital letters"
	},
	"@ae": {
		"prefix": "@ae{}",
		"body": [ "@ae{}" ],
		"description": "Non-ASCII lowercase 'AE' ligatures: 'æ'"
	},
	"@AE": {
		"prefix": "@AE{}",
		"body": [ "@AE{}" ],
		"description": "Non-ASCII lowercase 'AE' ligatures: 'Æ'"
	},
	"@afivepapter": {
		"prefix": "@afivepaper",
		"body": [ "@afivepaper" ],
		"description": "Specify document format for European size A5 paper"
	},
	"@afourlatex": {
		"prefix": "@afourlatex",
		"body": [ "@afourlatex" ],
		"description": "Specify document format for European size A4 paper (alternative)"
	},
	"@afourpaper": {
		"prefix": "@afourpaper",
		"body": [ "@afourpaper" ],
		"description": "Specify document format for European size A4 paper"
	},
	"@afourwide": {
		"prefix": "@afourwide",
		"body": [ "@afourwide" ],
		"description": "Specify document format for European size A4 paper in wide format"
	},
	"@alias": {
		"prefix": "@alias",
		"body": [ "@alias ${1:new} = ${2:existing}" ],
		"description": "Defines a new command to be just like an existing one"
	},
	"@allowcodebreaks": {
		"prefix": "@allowcodebreaks",
		"body": [ "@allowcodebreaks ${1|true,false|}" ],
		"description": "Allows breaking at '-' or '_' within @code"
	},
	"@ampchar": {
		"prefix": "@ampchar{}",
		"body": [ "@ampchar{}" ],
		"description": "Literal '&' character (only needed in definition commands)"
	},
	"@anchor": {
		"prefix": "@anchor{}",
		"body": [ "@anchor{${1:anchor}}" ],
		"description": ""
	},
	"@appendix": {
		"prefix": "@appendix",
		"body": [ "@appendix" ],
		"description": "Chapter-level structuring with letters as labels"
	},
	"@appendixsec": {
		"prefix": "@appendixsec",
		"body": [ "@appendixsec" ],
		"description": "Section-level structuring with letters as labels"
	},
	"@appendixsection": {
		"prefix": "@appendixsection",
		"body": [ "@appendixsection" ],
		"description": "Section-level structuring with letters as labels (Synonym for @appendixsec)"
	},
	"@appendixsubsection": {
		"prefix": "@appendixsubsection",
		"body": [ "@appendixsubsection" ],
		"description": "Subsection-level structuring with letters as labels"
	},
	"@appendixsubsubsection": {
		"prefix": "@appendixsubsubsection",
		"body": [ "@appendixsubsubsection" ],
		"description": "Subsubsection-level structuring with letters as labels"
	},
	"@arrow": {
		"prefix": "@arrow{}",
		"body": [ "@arrow{}" ],
		"description": "Non-ASCII right pointing arrow: '→'"
	},
	"@atchar": {
		"prefix": "@atchar{}",
		"body": [ "@atchar{}" ],
		"description": "Literal '@' character"
	},
	"@b": {
		"prefix": "@b{}",
		"body": [ "@b{}" ],
		"description": "Selects bold face"
	},
	"@backslashchar": {
		"prefix": "@backslashchar{}",
		"body": [ "@backslashchar{}" ],
		"description": "Literal '\\' character (only needed in bodies of user defined macros)"
	},
	"@bullet": {
		"prefix": "@bullet{}",
		"body": [ "@bullet{}" ],
		"description": "Non-ASCII bullet character: '•'"
	},
	"@bye": {
		"prefix": "@bye",
		"body": [ "@bye" ],
		"description": "Terminates Texinfo processing"
	},
	"@c": {
		"prefix": "@c",
		"body": [ "@c" ],
		"description": "Single line comment"
	},

	"multitable": {
		"prefix": "multitable",
		"body": [
			"@multitable @columnfractions .${1:0}",
			"@headitem ${2:head_items}",
			"",
			"@item ${3:items}",
			"",
			"@end multitable"
		],
		"description": "Create a multitable"
	}
}
