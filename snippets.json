{
	"Document Template": {
		"prefix": "document",
		"body": [
			"\\input texinfo @c -*-texinfo-*-",
			"@setfilename ${1:filename}",
			"@settitle ${2:program_title version}",
			"@documentdescription",
			"${3:document_description}",
			"@end documentdescription",
			"@documentlanguage ${4:ll[_CC]}",
			"",
			"@copying",
			"${5:program_description}",
			"",
			"Copyright @copyright{} ${CURRENT_YEAR:YYYY} ${6:your_name}",
			"",
			"@quotation",
			"${7:copying_notice}",
			"@end quotation",
			"@end copying",
			"",
			"@titlepage",
			"@title ${8:document_title}",
			"@author ${6}",
			"",
			"@page",
			"@vskip 0pt plus 1filll",
			"@insertcopying",
			"",
			"Published by ${6}",
			"@end titlepage",
			"",
			"@c @shortcontents",
			"@contents",
			"",
			"@ifnottex",
			"@node Top",
			"@top ${8}",
			"${5}",
			"@end ifnottex",
			"",
			"@menu",
			"* $9",
			"* Index:: Complete index.",
			"@end menu",
			"",
			"$0",
			"",
			"@node       Index",
			"@unnumbered Index",
			"@printindex cp",
			"",
			"@bye",
			""
		],
		"description": "Template for a document"
	},
	"Multitable": {
		"prefix": "multitable",
		"body": [
			"@multitable @columnfractions .${1:0}",
			"@headitem ${2:head_items}",
			"",
			"@item ${3:items}",
			"",
			"@end multitable"
		],
		"description": "Create a multitable"
	}
}
